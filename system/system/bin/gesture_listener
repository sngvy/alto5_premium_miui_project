#!/system/bin/sh
# Catch getevent ft5x06_ts signal and turn on display
(busybox script -q -c 'getevent /dev/input/event0' /dev/null | while read code; do echo "$code" | grep -q '^0001 0074 00000000.$' && echo performance > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor && input keyevent 26 && echo interactive > /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor; done) < /dev/null &>/dev/null &