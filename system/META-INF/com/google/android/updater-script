run_program("/cache/miui_update");
delete("/cache/miui_update");
set_metadata_recursive("/data/miui", "uid", 1000, "gid", 1000, "dmode", 0755, "fmode", 0644, "capabilities", 0x0);
delete_recursive("data/system/customized_icons");
ifelse(is_mounted("/data"), unmount("/data"));
ifelse(is_mounted("/cache"), unmount("/cache"));
ifelse(is_mounted("/system"), unmount("/system"));
run_program("/sbin/e2fsck", "-y", "/dev/block/mmcblk0p35");
ui_print("          *******************************");
ui_print("          ");
ui_print("          ****** УСТАНОВКА УСПЕШНО ******");
ui_print("          ********** ЗАВЕРШЕНА **********");
ui_print("          ");
ui_print("          *******************************");
ui_print("          ");
run_program("/sbin/reboot");
